{
  "name": "cheetah-capture",
  "version": "0.1.0",
  "description": "wasm 截取视频帧",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "dist",
    "types"
  ],
  "scripts": {
    "webpack-capture": "webpack --config ./script/webpack.config.js",
    "build-capture": "rollup --config",
    "gents": "npx tsc --declaration -p tsconfig.json --declarationDir ./types",
    "prettier": "prettier --write ./src/**.js",
    "eslint": "eslint --fix ./src/",
    "build": "bash ./script/build.sh",
    "commit": "git cz",
    "release": "standard-version --skip.commit && git commit --amend"
  },
  "author": "hixuanxuan",
  "homepage": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:wanwu/cheetah-capture.git"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "^7.18.6",
    "@ecomfe/eslint-config": "^7.4.0",
    "@types/emscripten": "^1.39.6",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "babel-loader": "^9.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "7.26.0",
    "prettier": "2.3.0",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "resolutions": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.20.2"
  },
  "dependencies": {
    "core-js": "3.6.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
